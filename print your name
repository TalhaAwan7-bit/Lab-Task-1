public class PrintDetails {
    public static void main(String[] args) {
        // Printing my details
        String name = "Talha Sarfraz";
        int age = 20;
        String city = "Abbottabad";

        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("City: " + city);
    }
}
